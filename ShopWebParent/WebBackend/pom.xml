		<artifactId>ShopWebParent</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>

	<artifactId>WebBackend</artifactId>
	<name>WebBackend</name>
	<description>Shop admin project</description>
	<properties>

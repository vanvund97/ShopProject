package com.evanshop.admin.setting;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.test.autoconfigure.jdbc.AutoConfigureTestDatabase;
import org.springframework.boot.test.autoconfigure.jdbc.AutoConfigureTestDatabase.Replace;
import org.springframework.boot.test.autoconfigure.orm.jpa.DataJpaTest;
import org.springframework.test.annotation.Rollback;

import com.evanshop.common.entity.SettingCategory;

@DataJpaTest
@AutoConfigureTestDatabase(replace = Replace.NONE)
@Rollback(false)
public class SettingRepoTests {
	
	@Autowired SettingRepository repo;
	
	public void findByTwoCatg() {
		repo.findByTwoCategory(SettingCategory.CURRENCY, SettingCategory.CURRENCY);
	}
}
